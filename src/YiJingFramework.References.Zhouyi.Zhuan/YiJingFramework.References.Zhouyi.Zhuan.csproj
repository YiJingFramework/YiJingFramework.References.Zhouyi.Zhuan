<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>yueyinqiu5990</Authors>
    <Company>YiJingFramework</Company>
    <Description>YiJingFramework is a C# library (group) about Yi Jing.</Description>
    <Copyright>Copyright Â© 2021 yueyinqiu5990. All rights reserved.</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>Taichi.png</PackageIcon>
    <RepositoryUrl>https://github.com/YiJingFramework/YiJingFramework.References.Zhouyi.Zhuan.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/YiJingFramework/YiJingFramework.References.Zhouyi.Zhuan/wiki</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>zhouyi;i-ching;yijing;yijingframework</PackageTags>
    <Version>3.0.0</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>obj\Release\net5.0\YiJingFramework.References.Zhouyi.Zhuan.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="YiJingFramework.References.Zhouyi" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\Taichi.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
